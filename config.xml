<?xml version='1.0' encoding='utf-8'?><widget     xmlns="http://www.w3.org/ns/widgets"     xmlns:gap="http://phonegap.com/ns/1.0"    xmlns:cdv="http://cordova.apache.org/ns/1.0"    id="de.mobc.bkdin"     version="1.0.5"    >    <name>Bkdin</name>    <description>Ionic Project bkdin</description>    <author email="dev@mobc.de" href="http://www.mobc.de">      2016 - mobC - Peter Busch - (Peter Busch)    </author>    <content src="index.html" />    <icon src="icon.png" />	<access origin="http://*/*" />
	<access origin="https://*/*" />
	<access origin="cdvfile://*" />
	<allow-navigation href="http://*/*" />
	<allow-navigation href="https://*/*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<feature name="http://api.phonegap.com/1.0/device" />
	<feature name="http://api.phonegap.com/1.0/camera" />
	<preference name="orientation" value="default" />
	<preference name="fullscreen" value="false" />
	<preference name="SplashScreen" value="splash" />
	<preference name="AutoHideSplashScreen" value="true" />
	<preference name="SplashScreenDelay" value="3000" />
	<plugin name="cordova-plugin-device" spec="^1.1.0" source="npm" />
	<plugin name="cordova-plugin-console" spec="^1.0.2" source="npm" />
	<plugin name="cordova-plugin-whitelist" spec="^1.2.0" source="npm" />
	<plugin name="cordova-plugin-splashscreen" spec="^3.0.0" source="npm" />
	<plugin name="ionic-plugin-keyboard" spec="^1.0.8" source="npm" />
	<plugin name="cordova-plugin-file" spec="^3.0.0" source="npm" />
	<plugin name="cordova-plugin-file-transfer" spec="^1.4.0" source="npm" />
</widget>